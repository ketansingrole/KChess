{
  "name": "KChess",
  "version": "1.0.11",
  "description": "A chess program for everyone",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ketansingrole/KChess.git"
  },
  "keywords": [
    "chess"
  ],
  "author": "Ketan Singrole <singroleketan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ketansingrole/KChess/issues"
  },
  "homepage": "https://github.com/ketansingrole/KChess#readme",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "chess.js": "^0.10.3",
    "electron-json-storage": "^4.1.8",
    "electron-log": "^4.2.0",
    "electron-updater": "^4.3.1",
    "jquery": "^3.5.1"
  },
  "build": {
    "appId": "org.KChess.KChess",
    "artifactName": "KChess-${version}.${ext}",
    "win": {
      "target": "nsis",
      "icon": "/build/bk.ico"
    },
    "linux": {
      "executableName": "kchess",
      "target": [
        "deb",
        "snap",
        "AppImage"
      ],
      "icon": "build/bK.png",
      "category": "Utility"
    }
  }
}
